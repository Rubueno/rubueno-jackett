[Unit]
Description=<%= @service_name %> Daemon
After=syslog.target network.target

[Service]
User=<%= @user %>
Group=<%= @group %>
Type=simple
SyslogIdentifier=jackett
Restart=always
RestartSec=5
ExecStart=<%= @base_path %>/<%= @executable %> --NoRestart

TimeoutStopSec=20
[Install]
WantedBy=multi-user.target
